<div class="modal-content">
    <div class="modal-header">
        <h5 class="modal-title">Credit Reference Log </h5>
        <button type="button" class="close hideModel1">
            ×
        </button>
    </div>
    <div class="modal-body">
        <table id="CreditReferenceTbl" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th class="th-sm">
                        <label>From Name</label>
                    </th>
                    <th class="th-sm">
                        <label>User Name</label>
                    </th>

                    <th class="th-sm">
                        <label>Old Credit Reference</label>
                    </th>
                    <th class="th-sm">
                        <label>New Credit Reference</label>
                    </th>

                </tr>
            </thead>
            <tbody>
            <td class="text-center" colspan="4">No records Found</td>
            </tbody>
        </table>
    </div>
</div>

@section PageJs{
    <script src="https://cdn.datatables.net/1.11.0/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.0/js/dataTables.bootstrap.min.js"></script>
    <script type="text/javascript">
        $(function () {

            $(".hideModel1").click(function () {
                $("#profitLossModalList").modal("hide");
            });
            $("#CreditReferenceTbl").DataTable();
            $(".dataTables_length").addClass("bs-select");
        });
    </script>
}