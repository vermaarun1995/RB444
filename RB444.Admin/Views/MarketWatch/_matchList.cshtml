@model IEnumerable<RB444.Data.Entities.Matches>

<table class="sportesTbl table table-striped table-dark">
    <thead>
        <tr>
            <th>S NO.</th>
            <th>Match Name</th>
            <th>Date</th>
            <th>ACTION</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Count() > 0)
        {
            int i = 1;
            foreach (var item in Model)
            {
                <tr>
                    <td scope="row">@i</td>
                    <td>
                        <a href="~/MarketWatch/TeamMarketWatch?marketId=@item.marketId&eventId=@item.eventId&SportId=@item.SportId">
                            @item.eventName
                        </a>
                    </td>
                    <td>@item.MatchDate</td>
                    <td>
                        <a href="javascript:;" onclick="showModel(@item.marketId,@item.SportId)">
                            <i class="fa fa-eye fa-lg"></i>
                        </a>
                    </td>
                </tr>
                i++;
            }
        }
    </tbody>
</table>

@*<button type="button" style="background-color: #6593db; border-color: #6593db; ">BetList</button>*@

<div class="modal" tabindex="-1" id="marketPositionModal">
    <div id="backLayAmount"></div>
</div>


@section PageJs{
    <script src="~/js/custom.js"></script>
}